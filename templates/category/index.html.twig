{% extends 'base.html.twig' %}

{% block title %} Category{% endblock %}

{% block body %}
<main class="container">
<section class="row">
<div class="col-12">
<h1 class="text-center">Category</h1>
<p class="text-center">Liste de tous les Category</p>
</div>
{% for category in category  %}
<div class="row col-4">
<div class="card h-100">
<div class="card-body">
<h3 class="card-title">{{ category.name }}</h3>
</div>
</div>
</div>
{% endfor %}
<div class="col-12">
{{ dump() }}
</section>
</main>
{% endblock %}